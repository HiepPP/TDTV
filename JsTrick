///Set time to sleep
function sleep(milliseconds) {
  var start = new Date().getTime();
  for (var i = 0; i < 1e7; i++) {
    if ((new Date().getTime() - start) > milliseconds){
      break;
    }
  }
}

///Trigger change event when document.ready
$("select#ProvinceId").trigger('change',function () {}

///Remove validate
$(this).rules('remove', 'required');
